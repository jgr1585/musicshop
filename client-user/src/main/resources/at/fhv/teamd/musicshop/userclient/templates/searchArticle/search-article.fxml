<?xml version="1.0" encoding="UTF-8"?>

<?import de.jensd.fx.glyphs.fontawesome.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<GridPane prefHeight="30.0" prefWidth="517.0" style="-fx-background-color: #000000FF;" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" fx:controller="at.fhv.teamd.musicshop.userclient.view.medium.SearchMediumController">
   <columnConstraints>
      <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="75.0" />
      <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="45.0" />
      <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="110.0" />
      <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="75.0" />
      <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
      <ColumnConstraints halignment="CENTER" hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
   </columnConstraints>
   <rowConstraints>
      <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
   </rowConstraints>
   <children>
      <Button mnemonicParsing="false" onAction="#addToCart" prefWidth="40.0" style="-fx-background-color: #547af9;" GridPane.columnIndex="5">
         <graphic>
            <FontAwesomeIconView glyphName="CART_PLUS" size="16" strokeType="OUTSIDE" strokeWidth="0.0" text="" />
         </graphic>
         <GridPane.margin>
            <Insets right="30.0" />
         </GridPane.margin></Button>
      <Button alignment="CENTER" contentDisplay="CENTER" mnemonicParsing="false" onAction="#order" prefWidth="40.0" style="-fx-background-color: #ffa500;" GridPane.columnIndex="4">
         <graphic>
            <FontAwesomeIconView glyphName="LIST_ALT" selectionEnd="0" size="16" strokeType="OUTSIDE" strokeWidth="0.0" text="" />
         </graphic>
         <GridPane.margin>
            <Insets left="50.0" />
         </GridPane.margin></Button>
      <Label fx:id="mediumPrice" alignment="CENTER" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="75.0" text="€€€" textFill="#26c303" GridPane.columnIndex="3">
         <font>
            <Font name="System Bold" size="12.0" />
         </font>
      </Label>
      <TextField fx:id="mediumAmount" alignment="CENTER" editable="false" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="35.0" style="-fx-background-color: #E6E6FA;" text="0" GridPane.columnIndex="1" />
      <Label fx:id="mediumType" alignment="CENTER" maxWidth="-Infinity" minWidth="-Infinity" prefHeight="16.0" prefWidth="75.0" text="Label" />
      <HBox alignment="CENTER" prefHeight="200.0" prefWidth="200.0" GridPane.columnIndex="2">
         <children>
            <Button layoutX="14.0" layoutY="2.0" mnemonicParsing="false" onAction="#reduceByOne" prefWidth="25.0" style="-fx-background-color: #FF0000; -fx-text-fill: #000000;" text="-" textFill="#101110" />
            <TextField fx:id="mediumAmountSelected" alignment="CENTER" layoutX="39.0" layoutY="2.0" maxWidth="-Infinity" minWidth="-Infinity" prefWidth="35.0" style="-fx-background-color: #FFFFFF;" text="0">
               <HBox.margin>
                  <Insets left="1.0" right="1.0" />
               </HBox.margin>
            </TextField>
            <Button layoutX="73.0" layoutY="1.0" mnemonicParsing="false" onAction="#increaseByOne" prefWidth="25.0" style="-fx-background-color: #05b789; -fx-text-fill: #000000;" text="+" />
         </children>
      </HBox>
   </children>
</GridPane>
